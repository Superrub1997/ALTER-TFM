<?xml version="1.0" encoding="UTF-8"?>
<Gameloop:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Gameloop="http://gameloop/1.0"
    xsi:schemaLocation="http://gameloop/1.0 Gameloop.ecore"
    win="HIT"
    lose="MISS"
    start="START"
    name="Skeet">
  <objInit name="target"
      class="target"
      posY="0.3"
      posZ="0.2"/>
  <objInit name="target1"
      class="target"
      posX="0.2"
      posY="0.2"
      posZ="-0.3"/>
  <objInit name="target2"
      class="target"
      posX="-0.5"
      posY="0.4"
      posZ="1.0"/>
  <objInit name="target3"
      class="target"
      posX="-0.6"
      posY="0.2"
      posZ="-0.4"/>
  <objInit name="target4"
      class="target"
      posX="-1.0"
      posY="0.3"
      posZ="-0.3"/>
  <objInit name="target5"
      class="target"
      posY="0.4"
      posZ="-0.5"/>
  <objInit name="player"
      class="player">
    <rules>
      <condition
          xsi:type="Gameloop:Arithmetic">
        <left
            xsi:type="Gameloop:AttributeValue"
            class="player"
            attribute="ammo"/>
        <right
            xsi:type="Gameloop:ConstantValue"
            value="0"/>
      </condition>
    </rules>
  </objInit>
  <score finish="-1"
      lives="3"/>
  <collisions
      classA="bullet"
      classB="target"
      action="//@actionsTriggers.0"/>
  <actionsTriggers
      name="hit"
      scoreChange="1"/>
  <actionsTriggers
      name="shoot"/>
  <gamepad name="shoot"
      trigger="//@actionsTriggers.1"/>
</Gameloop:Game>
